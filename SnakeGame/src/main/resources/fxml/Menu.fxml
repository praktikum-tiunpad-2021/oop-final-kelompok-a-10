<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.image.ImageView?>

<?import javafx.scene.image.Image?>
<Pane fx:id="menu" maxHeight="450.0" maxWidth="600.0" minHeight="450.0" minWidth="600.0" prefHeight="450.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.102" xmlns:fx="http://javafx.com/fxml/1">
   <ImageView fx:id="imageView" fitHeight="180.0" fitWidth="400.0" layoutX="140.0" layoutY="10.0" pickOnBounds="true"
              preserveRatio="true">
      <Image url="@../img/snake-menu.png"/>
   </ImageView> 
   <Button fx:id="startBtn" layoutX="220.0" layoutY="190.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="156.0"
           text="Play"/>
   <Button fx:id="levelBtn" layoutX="220.0" layoutY="250.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="156.0"
           text="Choose Level"/>
   <Button fx:id="leaderboardBtn" layoutX="220.0" layoutY="310.0" mnemonicParsing="false" prefHeight="25.0"
           prefWidth="156.0" text="Leaderboard"/>
   <Button fx:id="exitBtn" layoutX="220.0" layoutY="370.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="156.0"
           text="Save &amp; Exit"/>
</Pane>
